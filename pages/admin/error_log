    [titolo] => Cosa farete della vostra vita?
    [autore] => J. Krishnamurti
    [categoria_id] => 9
    [isbn] => 
    [copertina_url] => https://m.media-amazon.com/images/I/61Q3t64cVQL._SY522_.jpg
