    [titolo] => I Templari
    [autore] => Piers Paul Read
    [categoria_id] => 2
    [isbn] => 978-8854162778
    [copertina_url] => https://arice.leggere.it/arimg.ashx?e=9788854162778&k=gb&w=500&h=800
